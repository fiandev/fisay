{
  "name": "fisay",
  "version": "0.1.0-development",
  "description": "a css framework like tailwind css",
  "main": "build/src/index.js",
  "private": true,
  "repository": "https://github.com/fiandev/fisay.git",
  "bugs": "https://github.com/fiandev/fisay/issues",
  "homepage": "https://github.com/fiandev/fisay#readme",
  "scripts": {
    "build": "rm -rf ./build && tsc --build && echo 'build success :)'",
    "test": "fisay compile -i ./__test__/index.html -o ./__test__/css/style.css",
    "test2": "fisay compile -i ./__test__/test_folder -o ./__test__/css/style.css",
    "test3": "fisay compile -i ./__test__/ -o ./__test__/css/new.css -w",
    "dev": "./node_modules/.bin/nodemon"
  },
  "bin": {
    "fisay": "./bin/fisay"
  },
  "directories": {
    "src": "src"
  },
  "keywords": [
    "css",
    "framework",
    "js",
    "nodejs",
    "saas",
    "scss",
    "fisay"
  ],
  "author": "fiandev",
  "license": "MIT",
  "dependencies": {
    "chalk": "4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^9.4.1",
    "fs-extra": "^10.1.0",
    "fs-readdir-recursive": "^1.1.0",
    "sass": "^1.57.1",
    "node-watch": "^0.7.3",
    "require-all": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/require-all": "^3.0.3",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  }
}
